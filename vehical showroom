class Fourwheeler extends Vehicle
{
    public static void Four()
    {
        System.out.println("----------------------------------");
        System.out.println("Welcome to Fourheeler section ");
        System.out.println("we have 1)Petrol 2)Diesel 3)EV Cars");
        System.out.println("Enter your choice: ");
        Scanner sc= new Scanner(System.in); 
        int a= sc.nextInt(); 
        switch(a)
        {
            case 1:Petrol ref=new Petrol();
            ref.Four();
            break;
            case 2:Diesel ref1=new Diesel();
            ref1.Four();
            break;
            case 3:Ev ref2=new Ev();
            ref2.Four();
            break;
            default: System.out.println("----------------------------------");
            System.out.println("Wrong choice");

        }
    }
}

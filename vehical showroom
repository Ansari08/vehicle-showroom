public static void Test()
{
    System.out.println("----------------------------------");
    System.out.println("For test drive press any key");
    Scanner sc= new Scanner(System.in); 
    int a= sc.nextInt(); 
    Vehicle v =new Vehicle();
    v.Start();
    System.out.println("----------------------------------");
    System.out.println("1)To stop 2)Drift 3)details");
    Scanner sc1= new Scanner(System.in); 
    int b= sc.nextInt();
    if(b>=1 && b<=2)
    {
        switch(b)
    {
        case 1:v.Stop();
               break;
        case 2: v.Drift();
        break;
        case 3: Details();
        break;

    }
    }
    else
    {
        System.out.println("----------------------------------");
        System.out.println("wrong choice enter again");
        Two();
    }
    if(b>=1 && b<=2)
    {
        System.out.println("Enter any key to access details");
        int c= sc.nextInt();
        Details(); 
    }
    
}

        System.out.println("we have only one Three wheeler is Auto ");
        System.out.println("Enter one for Auto  ");
        Scanner sc= new Scanner(System.in); 
        int a= sc.nextInt(); 
        switch(a)
        {
            case 1:Auto ref=new Auto();
            ref.Three();
            break;
            default: System.out.println("----------------------------------");
            System.out.println("Wrong Choice");

        }
    }
}

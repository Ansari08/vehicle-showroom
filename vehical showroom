class Royalenfield extends Twowheeler
{
    public static void Two()
    {
        System.out.println("----------------------------------");
        System.out.println("Welcome to Royalenfield section ");
        System.out.println("we have two bike 1)Thunderbird 2)Classic ");
        Scanner sc= new Scanner(System.in); 
        int a= sc.nextInt(); 
        switch(a)
        {
            case 1:Thunderbird ref=new Thunderbird();
            ref.Two();
            break;
            case 2:Classic ref1=new Classic();
            ref1.Two();
            break;
            default: System.out.println("Wrong Choice");

        }
    }
}

import java.util.Scanner;
class Vehicle
{
    int vehiclenum;
    String name;
    String brand;
    String colour;
    int vehicleprice;
    //Assigning private details:
    private int Aadharnum;
    public  void setAadharnum(int Aadharnum)
    {
        this.Aadharnum=Aadharnum;
    }
    public int getAadharnum()
    {
        return Aadharnum;
    }
    private int customerid;
    public  void setcustomerid(int customerid )
    {
        this.customerid=customerid;
    }
    public int getcustomerid()
    {
        return customerid;
    }
    private int receiptno;
    public  void setreceiptno(int receiptnum)
    {
        this.receiptno=receiptnum;
    }
    public int getreceiptno()
    {
        return receiptno;
    }
    //assigning behaviour:
    public static void Start()
    {
        System.out.println("----------------------------------");
        System.out.println("you ride on vehicle");
    }
    public static void Stop()
    {
        System.out.println("----------------------------------");
        System.out.println("you stop vehicle");        
    }
    public static void Drift()
    {
        System.out.println("----------------------------------");
        System.out.println("you Drift vehicle"); 
        System.out.println("to stop enter any");
        Scanner sc= new Scanner(System.in); 
        int a= sc.nextInt();
        Stop();       
    }
}
class Twowheeler extends Vehicle
{
    public static void Two()
    {
        System.out.println("----------------------------------");
        System.out.println("Welcome to Twowheeler section ");
        System.out.println("we have 1)Honda 2)Bajaj 3)TVS 4)RoyalEnfield 5)Yamaha 6)Firefox");
        Scanner sc= new Scanner(System.in);
        System.out.print("Enter your choice :");  
        int a= sc.nextInt(); 
        switch(a)
        {
            case 1:Honda ref=new Honda();
            ref.Two();
            break;
            case 2:Bajaj ref1=new Bajaj();
            ref1.Two();
            break;
            case 3:Tvs ref2=new Tvs();
            ref2.Two();
            break;
            case 4:Royalenfield ref3=new Royalenfield();
            ref3.Two();
            break;
            case 5:Yamaha ref4=new Yamaha();
            ref4.Two();
            break;
            case 6:Cycle ref5=new Cycle();
            ref5.Two();
            break;
            default: System.out.println("Wrong choice");

        }
    }
}
class Cycle extends Twowheeler
{
    public static void Two()
    {
        System.out.println("----------------------------------");
        System.out.println("Welcome to Cycle section ");
        System.out.println("we have one Cycle 1)Firefox 2)For Twowheeler Section ");
        Scanner sc= new Scanner(System.in); 
        int a= sc.nextInt(); 
        switch(a)
        {
            case 1:Firefox ref=new Firefox();
            ref.Two();
            break;
            case 2:Twowheeler ref1=new Twowheeler();
            ref1.Two();
            break;
            default: System.out.println("Wrong Choice");
        }
    }   
}
class Firefox extends Cycle
{
    public static void Two()
    {
        System.out.println("----------------------------------");
        System.out.println("****Firefox****");
        System.out.println("1)to confirm or 2)Twowheeler");
        Scanner sc= new Scanner(System.in); 
        int a= sc.nextInt(); 
        switch(a)
        {
            case 1:Two1();
            break;
            case 2:Twowheeler ref1=new Twowheeler();
            ref1.Two();
            break;
            default: System.out.println("Wrong Choice");
        }
    } 
    public static void Two1()
   {
    System.out.println("----------------------------------");
    System.out.println("1)for test drive or 2)personal details");
    Scanner sc= new Scanner(System.in); 
    int a= sc.nextInt();
    switch(a)
    {
        case 1:Test();
        break;
        case 2:Details();
        break;
        default:System.out.println("----------------------------------");
         System.out.println("Wrong Choice Try again");
    }
    }
    public static void Test()
    {
        System.out.println("----------------------------------");
        System.out.println("For test drive press any key");
        Scanner sc= new Scanner(System.in); 
        int a= sc.nextInt(); 
        Vehicle v =new Vehicle();
        v.Start();
        System.out.println("----------------------------------");
        System.out.println("1) To stop 2)For Drift and 3)For details");
        Scanner sc1= new Scanner(System.in); 
        int b= sc.nextInt();
        if(b>=1 && b<=2)
        {
            switch(b)
        {
            case 1:v.Stop();
                   break;
            case 2: v.Drift();
            break;
            case 3: Details();
            break;
        }
        } 
        else
        {
            System.out.println("----------------------------------");
            System.out.println("wrong choice enter again");
            Two();
        }
    }
    private int password=11;
    public int getpassword()
    {
        return password;
    }
    public static void Details()
    {

        Vehicle P=new Vehicle();
        P.vehiclenum=0000;
        P.name="Sheela";
        P.brand="Firefox";
        P.colour="Black";
        P.vehicleprice=21123;
        System.out.println("Vehiclenum:"+P.vehiclenum);
        System.out.println("Name:"+P.name);
        System.out.println("Brand:"+P.brand);
        System.out.println("Colour:"+P.colour);
        System.out.println("vehicleprice:"+P.vehicleprice);
        System.out.println("----------------------------------");
        System.out.println("To access private details please enter password (password=11)");
        Scanner sc= new Scanner(System.in); 
        int a= sc.nextInt();
        Splendor p=new Splendor();
        int b=p.getpassword();
        P.setreceiptno(148795); 
        P.setcustomerid(89344645);
        P.setAadharnum(875873254);
        if(a==b)
        {
            System.out.println("----------------------------------");
            System.out.println("Receiptno: "+P.getreceiptno());
            System.out.println("Customerid: "+P.getcustomerid()); 
            System.out.println("Aadharnum: "+P.getAadharnum());
            Buy();
        }
        else
        {
            System.out.println("----------xoxoxoxoxoxox------------");
            System.out.println("Wrong password enter again"); 
        }

    }
    public static void Buy()
    {
        System.out.println("----------------------------------");
        System.out.println("1)To Buy Enter any key)Exit Showroom");
        Scanner sc= new Scanner(System.in); 
        int a= sc.nextInt();
        if(a==1)
        {
            System.out.println("--Thank You you BUY Frefox----------------");
        }
        else
        {
            System.out.println("xoxoxox---------Visit again-------xoxox");
        }
    }
}

class Tesla extends Ev
{
    public static void Four()
    {
        System.out.println("----------------------------------");
        System.out.println("****Tesla****");
        System.out.println("1)Confirm 2)Other Company");
        Scanner sc= new Scanner(System.in); 
        int a= sc.nextInt(); 
        switch(a)
        {
            case 1:Four1();
            break;
            case 2:Fourwheeler ref2=new Fourwheeler();
            ref2.Four();
            break;
            default: System.out.println("Wrong Choice");

        }
    }
    public static void Four1()
{
    System.out.println("----------------------------------");
    System.out.println("1)for test drive or 2)personal details");
    Scanner sc= new Scanner(System.in); 
    int a= sc.nextInt();
    switch(a)
    {
        case 1:Test();
        break;
        case 2:Details();
        break;
        default: System.out.println("----------------------------------");
        System.out.println("Wrong Choice Try again");
    }
}
public static void Test()
{
    System.out.println("----------------------------------");
    System.out.println("For test drive press any key");
    Scanner sc= new Scanner(System.in); 
    int a= sc.nextInt(); 
    Vehicle v =new Vehicle();
    v.Start();
    System.out.println("----------------------------------");
    System.out.println("1)To stop 2)Drift 3)Details");
    Scanner sc1= new Scanner(System.in); 
    int b= sc.nextInt();
    if(b>=1 && b<=2)
    {
        switch(b)
    {
        case 1:v.Stop();
               break;
        case 2: v.Drift();
        break;
        case 3: Details();
        break;

    }
    }
    else
    {
        System.out.println("----------------------------------");
        System.out.println("wrong choice enter again");
        Four();
    }
    if(b>=1 && b<=2)
    {
        System.out.println("Enter any key to access details");
        int c= sc.nextInt();
        Details(); 
    }
}

class Driver1
{

    public static void main(String[]args)
    {
        System.out.println("----------------------------------");
        System.out.println("Welcome to all in One Showroom");
        Scanner sc= new Scanner(System.in);    //System.in is a standard input stream  
        System.out.println("Enter your choice 1)Twowheeler 2)Threewheeler 3)Fourwheeler 4)Eightwheeler :"); 
        System.out.println("----------------------------------"); 
        int a= sc.nextInt(); 
        switch(a)
        {
            case 1:Twowheeler ref=new Twowheeler();
            ref.Two();
            break;
            case 2:Threewheeler ref1=new Threewheeler();
            ref1.Three();
            break;
            case 3:Fourwheeler ref2=new Fourwheeler();
            ref2.Four();
            break;
            case 4:Eightwheeler ref3=new Eightwheeler();
            ref3.Eight();
            break;
            default: System.out.println("Wrong choice");
                    break; 
        } 
        System.out.println("----------------------------------");
        System.out.println("Thank You So Much Visit again");  
        System.out.println("----------------------------------");             
    }    
}

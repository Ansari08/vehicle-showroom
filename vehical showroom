class Tvs extends Twowheeler
{
    public static void Two()
    {
        System.out.println("----------------------------------");
        System.out.println("Welcome to TVS section ");
        System.out.println("we have two bike 1)Apache 2)Jupiter ");
        Scanner sc= new Scanner(System.in); 
        int a= sc.nextInt(); 
        switch(a)
        {
            case 1:Apache ref=new Apache();
            ref.Two();
            break;
            case 2:Jupiter ref1=new Jupiter();
            ref1.Two();
            break;
            default: System.out.println("Wrong Choice");

        }
    }
